<h1>New List</h1>

<%= form_for :list, url: lists_path do |f| %>
  <p>
    <%= f.label :nombre %><br>
    <%= f.text_field :nombre %>
  </p>
 
  <p>
    <%= f.label :idiomaNativo %><br>
    <%= f.text_area :idiomaNativo %>
  </p>
  
  <p>
    <%= f.label :idiomaForaneo %><br>
    <%= f.text_area :idiomaForaneo %>
  </p>

  <%= f.hidden_field :user_id %>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
